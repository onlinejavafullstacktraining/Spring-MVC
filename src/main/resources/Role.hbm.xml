<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.spring.mvc">
    <class name="Role" table="user_roles">
        <id name="roleId" type="long">
            <column name="role_id" />
            <generator class="identity" />
        </id>
        <many-to-one name="login" class="Login" fetch="select" column="LOGIN_ID" not-null="true">
            <!-- <column name="userName" length="45" not-null="true" /> -->
        </many-to-one>
        <property name="role" type="string">
            <column name="role" length="45" not-null="true" />
        </property>
    </class>
</hibernate-mapping>